{
    "name": "Serial Sketch Uploader for bitbloq",
    "description": "Serial Sketch Uploader for bitbloq",
    "author": "bitbloq developer team - bq",
    "version": "0.0.1",
    "manifest_version": 2,
    "minimum_chrome_version": "23",
    "permissions": ["serial"],
    "app": {
        "background": {
            "scripts": ["js/launch.js"]
        }
    },
    "icons": {
        "16": "img/icon_16.png",
        "128": "img/icon_128.png"
    },
    "externally_connectable": {
        "matches": ["http://localhost:9000/*"]
    },
    "url_handlers": {
        "launcher": {
            "matches": [
                "http://localhost:9000/*"
            ],
            "title": "Launcher Serial Uploader"
        }
    }

}