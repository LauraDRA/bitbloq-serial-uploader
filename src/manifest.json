{
    "name": "bitbloq Serial Uploader",
    "description": "Serial Uploader for bitbloq",
    "author": "bitbloq developer team - bq",
    "version": "0.1.0",
    "manifest_version": 2,
    "minimum_chrome_version": "23",
    "default_locale": "es",
    "permissions": ["serial"],
    "app": {
        "background": {
            "scripts": ["launch.js"]
        }
    },
    "icons": {
        "16": "img/icon_16.png",
        "128": "img/icon_128.png"
    },
    "externally_connectable": {
        "matches": ["http://bitbloq.bq.com/*", "http://bitbloq.es/*", "http://localhost:9000/*"]
    }

}
