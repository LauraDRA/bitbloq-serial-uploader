{
    "name": "bitbloq Serial Uploader",
    "short_name": "bitbloqSU",
    "description": "Serial Uploader for bitbloq",
    "author": [{
        "name": "Sergio Morcuende",
        "email": "sergio.morcuende@bq.com"
    }, {
        "name": "Irene Sanz",
        "email": "irene.sanz@bq.com"
    }],
    "version": "0.1.4",
    "manifest_version": 2,
    "minimum_chrome_version": "23",
    "default_locale": "es",
    "permissions": ["serial"],
    "app": {
        "background": {
            "scripts": ["launch.js"]
        }
    },
    "icons": {
        "16": "img/icon_16.png",
        "128": "img/icon_128.png"
    },
    "externally_connectable": {
        "matches": ["http://localhost:9000/*", "http://bitbloq.bq.com/*", "http://www.bitbloq.es/*", "http://bitbloq.es/*", "http://bitbloq.org/*", "http://www.bitbloq.org/*", "http://int-bitbloq.com.s3-website-eu-west-1.amazonaws.com/*", "http://qa-bitbloq.com.s3-website-eu-west-1.amazonaws.com/*", "http://next-bitbloq.com.s3-website-eu-west-1.amazonaws.com/*", "http://staging-bitbloq.com.s3-website-eu-west-1.amazonaws.com/*", "http://prod-bitbloq.com.s3-website-eu-west-1.amazonaws.com/*"]
    },
    "offline_enabled": true

}
